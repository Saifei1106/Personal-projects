n                <i\r\n                  class=\"reply iconfont icon-huifu\"\r\n                  @click=\"parentReplyClick(item)\"\r\n                ></i>\r\n              </div>\r\n            </div>\r\n          </div>\r\n          <div\r\n            class=\"child\"\r\n            v-for=\"(childItem,index) in item.children\"\r\n            :key=\"childItem._id\"\r\n          >\r\n            <div\r\n              class=\"left\"\r\n              :style=\"{backgroundImage: `url(${baseURL}${childItem.author.photo})`}\"\r\n            ></div>\r\n            <div class=\"right\">\r\n              <div class=\"name-time\">\r\n                <span>{{ childItem.author.user }}</span>\r\n                <span>{{ formatDate(childItem.date) }}</span>\r\n              </div>\r\n              <div class=\"message\">\r\n                <span>@{{ childItem.replyUser.user }}</span>{{ childItem.text }}\r\n              </div>\r\n              <div class=\"like-reply\">\r\n                <i\r\n                  class=\"like iconfont icon-xinaixin\"\r\n                  :class=\"{\r\n                    isLike: userInfo._id && childItem.likes.includes(userInfo._id)\r\n                  }\"\r\n                  @click=\"childLikeClick(item._id, childItem._id, index)\"\r\n                ></i>\r\n                <span>{{ childItem.likes.length }}</span>\r\n                <i\r\n                  class=\"reply iconfont icon-huifu\"\r\n                  @click=\"childReplyClick(item,childItem)\"\r\n                ></i>\r\n              </div>\r\n            </div>\r\n          </div>\r\n          <div\r\n            class=\"replyInput\"\r\n            :class=\"{\r\n              show: item.ifShowReply\r\n            }\"\r\n          >\r\n            <el-input\r\n              size=\"mini\"\r\n              :placeholder=\"`@${item.replyUser.user}`\"\r\n              v-model=\"item.replyTxt\"\r\n            ></el-input>\r\n            <el-button\r\n              type=\"primary\"\r\n              size=\"mini\"\r\n              @click=\"replySubmit(item)\"\r\n            >Êèê‰∫§\r\n            </el-button>\r\n          </div>\r\n        </li>\r\n      </ul>\r\n    </article>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {mapState} from 'vuex'\r\n\r\nexport default {\r\n  name: \"Message\",\r\n  data() {\r\n    return {\r\n      //ÁïôË®ÄÂÜÖÂÆπ\r\n      textarea: \"\",\r\n      //Áî®‰∫éÂ±ïÁ§∫/ÈöêËóè emojiÈÄâÊã©Ê°Ü\r\n      ifShowEmoji: false,\r\n      //emojiÊï∞ÁªÑ\r\n      emoji: [\r\n        \"üòÉ\", \"üòÅ\", \"üòÇ\", \"üòä\", \"üôÉ\", \"ü•∞\", \"üòò\", \"ü§≠\", \"ü§î\", \"üòè\",\r\n        \"üòµ\", \"üòü\", \"üôÅ\", \"üò≥\", \"üò∞\", \"üò≠\", \"üòñ\", \"üò°\", \"üò†\", \"üòà\",\r\n        \"ü§°\", \"‚ò†Ô∏è\", \"üëª\", \"üôà\", \"üôâ\", \"‚ù§Ô∏è\", \"üíñ\", \"üí¢\", \"üí£\", \"üí§\",\r\n        \"üëã\", \"üëç\", \"üëå\", \"ü§û\", \"ü§ü\", \"ü§ô\", \"üëä\", \"üñï\", \"ü§ù\", \"üôè\",\r\n        \"üôÖ\", \"üôÖ‚Äç‚ôÇÔ∏è\", \"üë®‚Äç‚úàÔ∏è\", \"üë©‚Äç‚úàÔ∏è\", \"üë∑\", \"üë∑‚Äç‚ôÄÔ∏è\", \"ü§¶\", \"ü§¶‚Äç‚ôÇÔ∏è\", \"üë¥\", \"üßì\",\r\n        \"üê¥\", \"üêÆ\", \"üêñ\", \"üêë\", \"üêì\", \"ü¶ö\", \"üê≤\", \"üê≥\", \"üêü\", \"üêå\",\r\n      ],\r\n      //ÊâÄÊúâÁïôË®ÄÂàóË°®\r\n      msgList: []\r\n    }\r\n  },\r\n  computed: {\r\n    ...mapState(['userInfo'])\r\n  },\r\n  methods: {\r\n    //ÊòæÁ§∫Ë°®ÊÉÖÊ°Ü\r\n    showEmoji() {\r\n      this.ifShowEmoji = !this.ifShowEmoji\r\n    },\r\n\r\n    //Ë°®ÊÉÖÂõæÊ†áÁÇπÂáª\r\n    clickEmoji(item) {\r\n      if (this.textarea.length > 98) return\r\n      this.textarea += item\r\n    },\r\n\r\n    //Êèê‰∫§ÁïôË®Ä\r\n    async submit() {\r\n      //Ê≤°ÊúâÁôªÂΩïÁöÑËØùÔºåÁªôÊèêÁ§∫\r\n      if (!this.userInfo.user) {\r\n        return this.$message.error(\"ËØ∑ÂÖàÁôªÂΩï\")\r\n      }\r\n\r\n      //ÁïôË®ÄÂÜÖÂÆπ‰∏∫Á©∫\r\n      let val = this.textarea.trim()\r\n      if (!val) {\r\n        return this.$message.error(\"ËØ∑ËæìÂÖ•ÁïôË®ÄÂÜÖÂÆπ\")\r\n      }\r\n\r\n      //ÂèëÈÄÅÂà∞ÂêéÁ´Ø\r\n      let {data} = await this.$axios({\r\n        method: \"POST\",\r\n        url: \"/msg/submit\",\r\n        data: {val}\r\n      })\r\n      //Â§±Ë¥•\r\n      if (data.code) {\r\n        return this.$message.error(data.msg)\r\n      }\r\n      //ÊàêÂäü\r\n      this.$message.success(\"ÁïôË®ÄÊàêÂäü\") //ÊèêÁ§∫\r\n      this.textarea = \"\" //ÂÜÖÂÆπÊ∏ÖÁ©∫\r\n      this.getMsg() //ÈáçÊñ∞ËØ∑Ê±ÇÁïôË®ÄÊï∞ÊçÆ\r\n\r\n    },\r\n\r\n    //ËØ∑Ê±ÇÊâÄÊúâÁïôË®ÄÊï∞ÊçÆ\r\n    async getMsg() {\r\n      let {data} = await this.$axios({\r\n        method: \"GET\",\r\n        url: \"/get/msg\"\r\n      })\r\n\r\n      let d = data.data\r\n      d.forEach(item => {\r\n        item.ifShowReply = false //Áî®‰∫éÊéßÂà∂ÂõûÂ§çÊ°ÜÊòæ‰∏çÊòæÁ§∫\r\n        item.replyTxt = \"\" //ÂõûÂ§çÊ°ÜÁöÑÂÜÖÂÆπÁªëÂÆö\r\n        item.replyUser = item.author //ÊèêÁ§∫Ê°ÜÊñáÂ≠óÂÜÖÂÆπ\r\n      })\r\n\r\n      this.msgList = d\r\n    },\r\n\r\n    //ËΩ¨Êç¢Êó•ÊúüÊòæÁ§∫\r\n    formatDate(date) {\r\n      let d = new Date(date),\r\n        YY = d.getFullYear(),\r\n        MM = d.getMonth() + 1,\r\n        DD = d.getDate(),\r\n        hh = d.getHours(),\r\n        mm = d.getMinutes(),\r\n        ss = d.getSeconds()\r\n\r\n      MM = (MM < 10 ? \"0\" : \"\") + MM\r\n      DD = (DD < 10 ? \"0\" : \"\") + DD\r\n      hh = (hh < 10 ? \"0\" : \"\") + hh\r\n      mm = (mm < 10 ? \"0\" : \"\") + mm\r\n      ss = (ss < 10 ? \"0\" : \"\") + ss\r\n\r\n      return `${YY}-${MM}-${DD} ${hh}:${mm}:${ss}`\r\n    },\r\n\r\n    //Áà∂Á∫ßÁÇπËµûÊåâÈíÆÁÇπÂáª\r\n    async parentLikeClick(id) {\r\n      //Âà§Êñ≠Áî®Êà∑ÊòØÂê¶ÁôªÂΩï\r\n      if (!this.userInfo.user) {\r\n        return this.$message.error(\"ËØ∑ÂÖàÁôªÂΩï\")\r\n      }\r\n\r\n      //Â∑≤ÁôªÂΩï\r\n      let {data} = await this.$axios({\r\n        method: \"POST\",\r\n        url: \"/msg/like/parent\",\r\n        data: {id} //‰º†ÈÄíËØÑËÆ∫ÁöÑidÁªôÂêéÁ´Ø\r\n      })\r\n\r\n      //Êìç‰ΩúÂ§±Ë¥•\r\n      if (data.code) {\r\n        return this.$message.error(data.msg)\r\n      }\r\n\r\n      //Êìç‰ΩúÊàêÂäü\r\n      //ÈáçÊñ∞ËØ∑Ê±ÇËØÑËÆ∫Êï∞ÊçÆ\r\n      this.getMsg()\r\n\r\n    },\r\n    //Â≠êÁ∫ßÁÇπËµûÊåâÈíÆÁÇπÂáª\r\n    async childLikeClick(pID, cID, cIndex) {\r\n      //Âà§Êñ≠Áî®Êà∑ÊòØÂê¶ÁôªÂΩï\r\n      if (!this.userInfo.user) {\r\n        return this.$message.error(\"ËØ∑ÂÖàÁôªÂΩï\")\r\n      }\r\n\r\n      //ÂèëÈÄÅËØ∑Ê±Ç\r\n      let {data} = await this.$axios({\r\n        method: \"POST\",\r\n        url: \"/msg/like/child\",\r\n        data: {pID, cID, cIndex}\r\n      })\r\n\r\n      if (data.code) {\r\n        return this.$message.error(data.msg)\r\n      }\r\n\r\n      this.getMsg()\r\n\r\n    },\r\n\r\n    //ÂõûÂ§çÊèê‰∫§\r\n    async replySubmit(item) {\r\n      //Âà§Êñ≠Áî®Êà∑ÊòØÂê¶ÁôªÂΩï\r\n      if (!this.userInfo.user) {\r\n        return this.$message.error(\"ËØ∑ÂÖàÁôªÂΩï\")\r\n      }\r\n\r\n      let text = item.replyTxt.trim()\r\n\r\n      //È™åËØÅÊï∞ÊçÆÊ†ºÂºè\r\n      if (!text) {\r\n        return this.$message.error(\"ËØ∑ËæìÂÖ•ÂõûÂ§çÂÜÖÂÆπ\")\r\n      }\r\n      if (text.length > 100) {\r\n        return this.$message.error(\"ÂõûÂ§çÂ≠óÊï∞‰∏çËÉΩË∂ÖËøá100\")\r\n      }\r\n\r\n      //ÂèëÈÄÅÊï∞ÊçÆÂà∞ÂêéÁ´Ø\r\n      let {data} = await this.$axios({\r\n        method: \"POST\",\r\n        url: \"/msg/reply/submit\",\r\n        data: {\r\n          id: item._id,\r\n          text,\r\n          replyUser: item.replyUser._id\r\n        }\r\n      })\r\n\r\n      if (data.code) {\r\n        return this.$message.error(data.msg)\r\n      }\r\n\r\n      this.$message.success(\"ÂõûÂ§çÊàêÂäü\")\r\n      this.getMsg()\r\n\r\n    },\r\n\r\n    //ÂõûÂ§çÊåâÈíÆÁöÑÁÇπÂáª\r\n    parentReplyClick(item) {\r\n      //Âà§Êñ≠Áî®Êà∑ÊòØÂê¶ÁôªÂΩï\r\n      if (!this.userInfo.user) {\r\n        return this.$message.error(\"ËØ∑ÂÖàÁôªÂΩï\")\r\n      }\r\n      //ÈÄöËøáÂà§Êñ≠ @ ÁöÑÂêçÂ≠ó‰∏ÄÊ†∑‰∏ç‰∏ÄÊ†∑ÔºåÊù•ÂÜ≥ÂÆöÊòØÂê¶ÊòØÂ±ïÂºÄ\r\n      if (item.author._id !== item.replyUser._id){\r\n        item.ifShowReply = true\r\n      }else{\r\n        item.ifShowReply = !item.ifShowReply\r\n      }\r\n      //ÊîπÂèòreplyUser\r\n      item.replyUser = item.author\r\n    },\r\n\r\n    //Â≠êÂõûÂ§çÊåâÈíÆÁÇπÂáª\r\n    childReplyClick(item, childItem) {\r\n      //Âà§Êñ≠Áî®Êà∑ÊòØÂê¶ÁôªÂΩï\r\n      if (!this.userInfo.user) {\r\n        return this.$message.error(\"ËØ∑ÂÖàÁôªÂΩï\")\r\n      }\r\n      //ÈÄöËøáÂà§Êñ≠ @ ÁöÑÂêçÂ≠ó‰∏ÄÊ†∑‰∏ç‰∏ÄÊ†∑ÔºåÊù•ÂÜ≥ÂÆöÊòØÂê¶ÊòØÂ±ïÂºÄ\r\n      if (childItem.author._id !== item.replyUser._id){\r\n        item.ifShowReply = true\r\n      }else{\r\n        item.ifShowReply = !item.ifShowReply\r\n      }\r\n      //ÊîπÂèòreplyUser\r\n      item.replyUser = childItem.author\r\n    }\r\n  },\r\n  created() {\r\n    //ËØ∑Ê±ÇÁïôË®ÄÊï∞ÊçÆ\r\n    this.getMsg()\r\n  },\r\n  mounted() {\r\n    //ÁÇπÂáªdocumentÁöÑÊó∂ÂÄôemojiÊ∂àÂ§±\r\n    document.addEventListener(\"click\", () => {\r\n      this.ifShowEmoji = false\r\n    })\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n#Message {\r\n  article {\r\n    box-sizing: border-box;\r\n    width: 100%;\r\n    margin-bottom: 15px;\r\n    box-shadow: 0 0 4px #ddd;\r\n    padding: 25px 20px;\r\n    background-color: #fff;\r\n    font-family: Quicksand, Microsoft YaHei, sans-serif;\r\n\r\n    h2 {\r\n      margin-bottom: 15px;\r\n      font-size: 20px;\r\n      letter-spacing: 3px;\r\n      line-height: 24px;\r\n      border-left: 5px solid #73b899;\r\n      text-indent: 10px;\r\n    }\r\n\r\n    p {\r\n      text-indent: 2em;\r\n      color: #444;\r\n      font-size: 14px;\r\n      letter-spacing: 4px;\r\n      line-height: 30px;\r\n    }\r\n  }\r\n\r\n  article.a1 {\r\n    .el-textarea {\r\n      margin-top: 15px;\r\n    }\r\n\r\n    /deep/ textarea {\r\n      font-family: \"Quicksand\", \"ÂæÆËΩØÈõÖÈªë\";\r\n      font-size: 12px;\r\n      letter-spacing: 2px;\r\n      height: 70px;\r\n    }\r\n\r\n    .btn {\r\n      display: flex;\r\n      justify-content: space-between;\r\n      margin-top: 10px;\r\n\r\n      .left {\r\n    