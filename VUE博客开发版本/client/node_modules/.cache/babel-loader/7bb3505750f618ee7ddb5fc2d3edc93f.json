AA,2EAA6B,iBAAM,CAAN;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA,uBACvB,CADuB;AAAA;AAAA;AAAA;;AAAA;AAAA,yBAEN,MAAI,CAAC,MAAL,CAAY;AAC7B,oBAAA,MAAM,EAAE,MADqB;AAE7B,oBAAA,GAAG,EAAE,gBAFwB;AAG7B,oBAAA,IAAI,EAAE;AAAC,sBAAA,IAAI,EAAE,MAAI,CAAC,QAAL,CAAc;AAArB;AAHuB,mBAAZ,CAFM;;AAAA;AAAA;AAEpB,kBAAA,IAFoB,wBAEpB,IAFoB;;AAAA,uBASrB,IAAI,CAAC,IATgB;AAAA;AAAA;AAAA;;AAAA,mDAUhB,MAAI,CAAC,QAAL,CAAc,KAAd,CAAoB,IAAI,CAAC,GAAzB,CAVgB;;AAAA;AAazB;AACA,kBAAA,MAAI,CAAC,QAAL,CAAc,OAAd,CAAsB,SAAtB,EAdyB,CAcQ;;;AACjC,kBAAA,MAAI,CAAC,YAAL,CAAkB,IAAI,CAAC,IAAvB,EAfyB,CAeI;;;AAC7B,kBAAA,MAAI,CAAC,KAAL,CAAW,QAAX,CAAoB,WAApB,GAhByB,CAgBS;;;AAhBT;AAAA;;AAAA;AAAA,mDAmBlB,KAnBkB;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,SAA7B;;AAAA;AAAA;AAAA;AAAA;AAsBF;AAzBK;AAlCM,CAAf","sourcesContent":["<template>\r\n  <div class=\"UpdateName\">\r\n    <el-form\r\n      :model=\"userForm\"\r\n      status-icon\r\n      :rules=\"rules\"\r\n      ref=\"userForm\"\r\n      label-width=\"100px\"\r\n    >\r\n\r\n      <el-form-item label=\"当前用户名：\" label-width=\"100px\">\r\n        <span class=\"code\">{{userInfo.user}}</span>\r\n      </el-form-item>\r\n      <el-form-item label=\"新用户名：\" prop=\"user\" label-width=\"100px\">\r\n        <el-input v-model=\"userForm.user\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item label-width=\"100px\">\r\n        <el-button type=\"primary\" @click=\"submit\">确认修改</el-button>\r\n      </el-form-item>\r\n    </el-form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {mapState, mapMutations} from \"vuex\"\r\n\r\nexport default {\r\n  name: \"UpdateName\",\r\n  data() {\r\n    return {\r\n      userForm: {\r\n        user: \"\"\r\n      },\r\n      rules: {\r\n        user: [\r\n          {\r\n            validator: (rule, value, cb) => {\r\n              //长度验证\r\n              if (value.length < 2 || value.length > 8) {\r\n                cb(new Error(\"请输入2~8位的用户名\"))\r\n              }\r\n              //规则验证\r\n              if (!/^[\\w\\u4e00-\\u9fa5\\u0800-\\u4e00\\uac00-\\ud7ff]+$/.test(value)) {\r\n                cb(new Error(\"只允许 数字 字母 _ 中/日/韩文\"))\r\n              }\r\n              //不能和原来的一样\r\n              if (value === this.userInfo.user){\r\n                cb(new Error(\"用户名没有变化...\"))\r\n              }\r\n\r\n              cb()\r\n            }\r\n          }\r\n        ]\r\n      }\r\n    }\r\n  },\r\n  computed: {\r\n    ...mapState(['userInfo'])\r\n  },\r\n  methods: {\r\n    ...mapMutations(['loginSuccess']),\r\n    submit() {\r\n      this.$refs.userForm.validate(async v=>{\r\n        if (v){\r\n          let {data} = await this.$axios({\r\n            method: \"POST\",\r\n            url: \"/personal/user\",\r\n            data: {user: this.userForm.user}\r\n          })\r\n\r\n          //修改失败\r\n          if (data.code){\r\n            return this.$message.error(data.msg)\r\n          }\r\n\r\n          //修改成功\r\n          this.$message.success(\"用户名修改成功\") //提示\r\n          this.loginSuccess(data.data) //vuex更新\r\n          this.$refs.userForm.resetFields() //表单清空\r\n\r\n        }else{\r\n          return false\r\n        }\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.UpdateName {\r\n  span.code{\r\n    color: #bd4147;\r\n    background-color: rgba(27,31,35,.05);\r\n    font-weight: 700;\r\n  }\r\n}\r\n\r\n</style>\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n"],"sourceRoot":""}]}