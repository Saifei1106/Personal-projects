    })\r\n\r\n      if (data.code) {\r\n        return this.$message.error(data.msg)\r\n      }\r\n\r\n      this.contactData = data.data\r\n\r\n    },\r\n\r\n    //switch切换触发\r\n    async switchHandle(item) {\r\n      let {data} = await this.$axios({\r\n        method: \"POST\",\r\n        url: \"/adminServer/contact/read\",\r\n        data: {\r\n          id: item._id,\r\n          read: item.read\r\n        }\r\n      })\r\n      if (data.code){\r\n        return this.$message.error(data.msg)\r\n      }\r\n    }\r\n  },\r\n  created() {\r\n    this.getContactData()\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n"],"sourceRoot":""}]}