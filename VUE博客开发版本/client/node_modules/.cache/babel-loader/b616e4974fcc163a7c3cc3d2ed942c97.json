AA,OAAO,kCACF,YAAY,CAAC,CAAC,UAAD,CAAD,CADV;AAEL,IAAA,MAFK,oBAEI;AAAA;;AACP,WAAK,KAAL,CAAW,QAAX,CAAoB,QAApB;AAAA,2EAA6B,iBAAM,CAAN;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA,uBACvB,CADuB;AAAA;AAAA;AAAA;;AAAA;AAAA,yBAGN,MAAI,CAAC,MAAL,CAAY;AAC7B,oBAAA,MAAM,EAAE,MADqB;AAE7B,oBAAA,GAAG,EAAE,gBAFwB;AAG7B,oBAAA,IAAI,EAAE;AACJ,sBAAA,OAAO,EAAE,MAAI,CAAC,QAAL,CAAc,OADnB;AAEJ,sBAAA,IAAI,EAAE,MAAI,CAAC,QAAL,CAAc;AAFhB;AAHuB,mBAAZ,CAHM;;AAAA;AAAA;AAGpB,kBAAA,IAHoB,wBAGpB,IAHoB;;AAAA,uBAarB,IAAI,CAAC,IAbgB;AAAA;AAAA;AAAA;;AAAA,mDAchB,MAAI,CAAC,QAAL,CAAc,KAAd,CAAoB,IAAI,CAAC,GAAzB,CAdgB;;AAAA;AAiBzB;AACA,kBAAA,MAAI,CAAC,QAAL,CAAc,OAAd,CAAsB,cAAtB,EAlByB,CAkBa;;;AACtC,kBAAA,MAAI,CAAC,QAAL,GAnByB,CAmBT;;;AAChB,kBAAA,MAAI,CAAC,KAAL,CAAW,QAAX,CAAoB,WAApB,GApByB,CAoBS;;;AAClC,kBAAA,MAAI,CAAC,OAAL,CAAa,IAAb,CAAkB,GAAlB,EArByB,CAqBF;;;AArBE;AAAA;;AAAA;AAAA,mDAwBlB,KAxBkB;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,SAA7B;;AAAA;AAAA;AAAA;AAAA;AA2BF;AA9BK;AAxCM,CAAf","sourcesContent":["<template>\r\n  <div id=\"UpdatePass\">\r\n    <el-form\r\n      :model=\"passForm\"\r\n      status-icon\r\n      :rules=\"rules\"\r\n      ref=\"passForm\"\r\n      label-width=\"100px\"\r\n    >\r\n\r\n      <el-form-item label=\"原密码：\" prop=\"pass\" label-width=\"100px\">\r\n        <el-input type=\"password\" v-model=\"passForm.oldPass\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item label=\"新密码：\" prop=\"pass\" label-width=\"100px\">\r\n        <el-input type=\"password\" v-model=\"passForm.pass\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item label=\"确认新密码：\" prop=\"pass2\" label-width=\"100px\">\r\n        <el-input type=\"password\" v-model=\"passForm.pass2\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item label-width=\"100px\">\r\n        <el-button type=\"primary\" @click=\"submit\">确认修改</el-button>\r\n      </el-form-item>\r\n    </el-form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {mapMutations} from 'vuex'\r\nexport default {\r\n  name: \"UpdatePass\",\r\n  data() {\r\n    return {\r\n      passForm: {\r\n        oldPass: \"\",\r\n        pass: \"\",\r\n        pass2: \"\"\r\n      },\r\n      rules: {\r\n        pass: [\r\n          {\r\n            validator(rule, value, cb) {\r\n              //长度验证\r\n              if (value.length < 6 || value.length > 18) {\r\n                cb(new Error(\"请输入6~18位的密码\"))\r\n              }\r\n              //规则验证\r\n              if (!/^[\\w\\[\\]\\/\\\\~`|<>,.?;':\"{}!@#$%^&*()_+=-]+$/.test(value)) {\r\n                cb(new Error(\"不允许的密码字符\"))\r\n              }\r\n\r\n              cb()\r\n            },\r\n            trigger: \"input\"\r\n          }\r\n        ],\r\n        pass2: [\r\n          {\r\n            validator: (rule, value, cb) => {\r\n              if (this.passForm.pass !== value) {\r\n                cb(new Error(\"两次输入密码不一致\"))\r\n              }\r\n              cb()\r\n            }\r\n          }\r\n        ]\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n    ...mapMutations(['loginOut']),\r\n    submit() {\r\n      this.$refs.passForm.validate(async v=>{\r\n        if (v) {\r\n\r\n          let {data} = await this.$axios({\r\n            method: \"POST\",\r\n            url: \"/personal/pass\",\r\n            data: {\r\n              oldPass: this.passForm.oldPass,\r\n              pass: this.passForm.pass\r\n            }\r\n          })\r\n\r\n          //修改失败\r\n          if (data.code){\r\n            return this.$message.error(data.msg)\r\n          }\r\n\r\n          //修改成功\r\n          this.$message.success(\"密码修改成功，请重新登录\") //提示\r\n          this.loginOut() //vuex清理\r\n          this.$refs.passForm.resetFields() //清空表单\r\n          this.$router.push(\"/\") //返回首页\r\n\r\n        }else{\r\n          return false\r\n        }\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n"],"sourceRoot":""}]}